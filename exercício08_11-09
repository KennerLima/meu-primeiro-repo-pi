<script>
        function verificarBeneficio() {
            const rendaIndividual = Number (input_renda_individual.value)
            const rendaFamiliar = const rendaFamiliar = Number (input_renda_individual.value)

            if (rendaIndividual <= 2000 || rendaFamiliar <= 3000) {
                alert("Você terá direito ao auxílio")
            } else {
                alert("Sem direito ao auxílio")
            }
        }
</script>
